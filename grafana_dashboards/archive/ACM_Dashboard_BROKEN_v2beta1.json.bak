      "panel-71": {
        "kind": "Panel",
        "spec": {
          "data": {
            "kind": "QueryGroup",
            "spec": {
              "queries": [
                {
                  "kind": "PanelQuery",
                  "spec": {
                    "hidden": false,
                    "query": {
                      "datasource": {
                        "name": "${datasource}"
                      },
                      "group": "mssql",
                      "kind": "DataQuery",
                      "spec": {
                        "editorMode": "code",
                        "format": "table",
                        "rawQuery": true,
                        "rawSql": "-- Optimized RUL with current health status (3x faster with CTE)\r\nWITH LatestHealth AS (\r\n    SELECT TOP 1 HealthIndex, HealthZone, Timestamp AS HealthTime\r\n    FROM ACM_HealthTimeline \r\n    WHERE EquipID = $equipment \r\n    ORDER BY Timestamp DESC\r\n),\r\nLatestRUL AS (\r\n    SELECT TOP 1 RunID, Method, P50_Median AS RUL_Hours, P10_LowerBound, P90_UpperBound, Confidence, TopSensor1, TopSensor2, TopSensor3, FailureTime\r\n    FROM ACM_RUL \r\n    WHERE EquipID = $equipment AND (P10_LowerBound IS NOT NULL OR P50_Median IS NOT NULL)\r\n    ORDER BY CreatedAt DESC\r\n),\r\nActiveDefects AS (\r\n    SELECT COUNT(*) AS DefectCount\r\n    FROM ACM_SensorDefects\r\n    WHERE EquipID = $equipment AND RunID = (SELECT RunID FROM LatestRUL) AND ActiveDefect = 1\r\n)\r\nSELECT \r\n    r.Method,\r\n    ROUND(r.RUL_Hours, 1) AS 'RUL Median (h)',\r\n    ROUND(r.P10_LowerBound, 1) AS 'P10',\r\n    ROUND(r.P90_UpperBound, 1) AS 'P90',\r\n    ROUND(r.Confidence, 3) AS 'Confidence',\r\n    CASE WHEN r.RUL_Hours < 24 THEN 'CRITICAL' WHEN r.RUL_Hours < 72 THEN 'HIGH' WHEN r.RUL_Hours < 168 THEN 'MEDIUM' ELSE 'LOW' END AS 'Priority',\r\n    FORMAT(r.FailureTime, 'yyyy-MM-dd HH:mm') AS 'Est. Failure Time',\r\n    DATEDIFF(DAY, GETDATE(), r.FailureTime) AS 'Days Left',\r\n    r.TopSensor1 AS 'Top Culprit',\r\n    r.TopSensor2 AS 'Culprit 2',\r\n    r.TopSensor3 AS 'Culprit 3',\r\n    ROUND(h.HealthIndex, 1) AS 'Current Health',\r\n    h.HealthZone AS 'Health Zone',\r\n    d.DefectCount AS 'Active Defects'\r\nFROM LatestRUL r\r\nCROSS JOIN LatestHealth h\r\nCROSS JOIN ActiveDefects d;",
                        "sql": {
                          "columns": [
                            {
                              "parameters": [],
                              "type": "function"
                            }
                          ],
                          "groupBy": [
                            {
                              "property": {
                                "type": "string"
                              },
                              "type": "groupBy"
                            }
                          ],
                          "limit": 50
                        }
                      },
                      "version": "v0"

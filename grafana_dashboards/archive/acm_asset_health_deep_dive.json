      "id": 1,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "textMode": "value_and_name",
        "wideLayout": true
      },
      "targets": [
        {
          "datasource": {
            "type": "mssql",
            "uid": "${datasource}"
          },
          "format": "time_series",
          "rawSql": "SELECT\n  GETUTCDATE() AS time,\n  'Pct in ALERT (7d)' AS metric,\n  CASE WHEN COUNT(*) = 0 THEN 0 ELSE 100.0 * SUM(CASE WHEN HealthZone = 'ALERT' THEN 1 ELSE 0 END) / COUNT(*) END AS value\nFROM ACM_HealthTimeline_Latest\nWHERE EquipID = $equipment\n  AND Timestamp >= DATEADD(day, -7, GETUTCDATE())",
          "refId": "A"
        }
      ],
      "title": "Time in ALERT (7d)",
      "type": "stat"
    },

      "description": "Percent of points in ALERT zone over last 7 days from ACM_HealthTimeline_Latest.",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "#14b8a6",
                "value": null
              },
              {
                "color": "#f59e0b",
                "value": 10
              },
              {
                "color": "#f97316",
                "value": 20
              }
            ]
          },
          "unit": "percentunit",
          "links": [
            {
              "title": "Open Ops Command Center",
              "url": "/d/acm_ops_command_center/acm-ops-command-center?var-equipment=$equipment"
            }
          ]
        },
        "overrides": []
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0

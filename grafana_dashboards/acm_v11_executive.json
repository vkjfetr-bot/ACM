          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH LatestHealth AS (\n    SELECT EquipID, HealthZone\n    FROM ACM_HealthTimeline h\n    WHERE Timestamp = (\n        SELECT MAX(h2.Timestamp) \n        FROM ACM_HealthTimeline h2 \n        WHERE h2.EquipID = h.EquipID\n    )\n)\nSELECT \n    HealthZone AS metric,\n    COUNT(*) AS value\nFROM LatestHealth\nGROUP BY HealthZone\nORDER BY value DESC",

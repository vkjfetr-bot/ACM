      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 61 },
      "id": 62,
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,

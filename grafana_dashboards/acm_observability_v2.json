{
  "annotations": {
    "list": []
  },
  "description": "ACM Pipeline Observability - Traces, Logs, Metrics, Profiling",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "panels": [],
      "title": "RUN HISTORY",
      "type": "row"
    },
    {
      "datasource": { "type": "mssql", "uid": "mssql-ds" },
      "description": "Recent ACM pipeline runs",
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto"
          },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Outcome" },
            "properties": [
              {
                "id": "mappings",
                "value": [
                  { "type": "value", "options": { "HEALTHY": { "text": "HEALTHY", "color": "green" } } },
                  { "type": "value", "options": { "WARNING": { "text": "WARNING", "color": "yellow" } } },
                  { "type": "value", "options": { "CRITICAL": { "text": "CRITICAL", "color": "red" } } }
                ]
              }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Duration" },
            "properties": [{ "id": "unit", "value": "s" }]
          },
          {
            "matcher": { "id": "byName", "options": "RowsIn" },
            "properties": [{ "id": "unit", "value": "short" }]
          }
        ]
      },
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 1 },
      "id": 2,
      "options": {
        "showHeader": true,
        "cellHeight": "sm",
        "footer": { "show": false, "reducer": ["sum"] },
        "sortBy": [{ "displayName": "StartedAt", "desc": true }]
      },
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "mssql-ds" },
          "format": "table",
          "rawSql": "SELECT TOP 20\n  RunID,\n  EquipName AS Equipment,\n  HealthStatus AS Outcome,\n  StartedAt,\n  CompletedAt AS FinishedAt,\n  DurationSeconds AS Duration,\n  ScoreRowCount AS RowsIn,\n  ConfigSignature\nFROM ACM_Runs\nWHERE StartedAt BETWEEN $__timeFrom() AND $__timeTo()\nORDER BY StartedAt DESC",
          "refId": "A"
        }
      ],
      "title": "Recent Runs",
      "type": "table"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 7 },
      "id": 10,
      "panels": [],
      "title": "RESOURCE UTILIZATION",
      "type": "row"
    },
    {
      "datasource": { "type": "tempo", "uid": "tempo-ds" },
      "description": "Memory usage per pipeline phase extracted from trace spans",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisLabel": "Memory (MB)",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "auto",
            "spanNulls": 3600000,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "off" }
          },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] },
          "unit": "decmbytes"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 8 },
      "id": 11,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "datasource": { "type": "tempo", "uid": "tempo-ds" },
          "queryType": "traceqlmetrics",
          "query": "{span.acm.mem_mb > 0} | rate() by (resource.service.name)",
          "refId": "A"
        }
      ],
      "title": "Memory Usage (from Traces)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus-ds" },
      "description": "CPU utilization over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisLabel": "CPU %",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "auto",
            "spanNulls": 3600000,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "off" }
          },
          "mappings": [],
          "max": 100,
          "min": 0,
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] },
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 8 },
      "id": 12,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus-ds" },
          "expr": "acm_cpu_percent",
          "legendFormat": "{{equipment}}",
          "refId": "A"
        }
      ],
      "title": "CPU Utilization",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus-ds" },
      "description": "Memory RSS over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisLabel": "Memory (MB)",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "auto",
            "spanNulls": 3600000,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "off" }
          },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] },
          "unit": "decmbytes"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 8 },
      "id": 13,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus-ds" },
          "expr": "acm_memory_rss_mb",
          "legendFormat": "{{equipment}}",
          "refId": "A"
        }
      ],
      "title": "Memory RSS",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 16 },
      "id": 20,
      "panels": [],
      "title": "DISTRIBUTED TRACES",
      "type": "row"
    },
    {
      "datasource": { "type": "tempo", "uid": "tempo-ds" },
      "description": "Search and explore pipeline traces",
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 17 },
      "id": 21,
      "options": {},
      "targets": [
        {
          "datasource": { "type": "tempo", "uid": "tempo-ds" },
          "queryType": "traceql",
          "query": "{resource.service.name=\"acm-pipeline\"} | select(span.acm.mem_mb, span.acm.duration_s, span.acm.equipment)",
          "limit": 20,
          "refId": "A"
        }
      ],
      "title": "Trace Search",
      "type": "traces"
    },
    {

        {
          "datasource": { "type": "grafana-pyroscope-datasource", "uid": "pyroscope-ds" },
          "profileTypeId": "memory:alloc_space:bytes:space:bytes",
          "labelSelector": "{service_name=\"acm-pipeline\"}",
          "queryType": "profile",
          "refId": "A"
        }
      ],

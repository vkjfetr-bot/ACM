      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 70 },
      "id": 71,
      "options": {
        "legend": { "calcs": ["last", "max"], "displayMode": "table", "placement": "right", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "mssql-ds" },
          "format": "time_series",
          "rawSql": "SELECT\n  Timestamp AS time,\n  ar1_z AS 'AR1',\n  pca_spe_z AS 'PCA_SPE',\n  pca_t2_z AS 'PCA_T2',\n  iforest_z AS 'IForest',\n  gmm_z AS 'GMM',\n  omr_z AS 'OMR',\n  fused_z AS 'Fused'\nFROM ACM_Scores_Wide\nWHERE Timestamp BETWEEN $__timeFrom() AND $__timeTo()\n  AND EquipID = (SELECT TOP 1 EquipID FROM ACM_Runs ORDER BY StartedAt DESC)\nORDER BY Timestamp ASC",
          "refId": "A"
        }
      ],
      "title": "Detector Z-Scores (Latest Run)",
      "type": "timeseries"
    }

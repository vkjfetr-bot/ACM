                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    { "color": "green", "value": null },
                    { "color": "yellow", "value": 1.5 },
                    { "color": "orange", "value": 2.5 },
                    { "color": "red", "value": 3.5 }
                  ]
                }
              }
            ]
          }
        ]

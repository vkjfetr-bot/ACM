      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  d.EpisodeID,\n  e.StartTime AS 'Start Time',\n  e.EndTime AS 'End Time',\n  d.duration_h AS 'Duration (h)',\n  e.Severity,\n  d.peak_fused_z AS 'Max Fused Z',\n  d.avg_fused_z AS 'Avg Fused Z',\n  d.top_detector AS 'Top Detector'\nFROM ACM_EpisodeDiagnostics d\nLEFT JOIN ACM_Anomaly_Events e ON d.EpisodeID = e.Id\nWHERE d.EquipID = $equipment\nORDER BY e.StartTime DESC",
          "refId": "A"
        }
      ],
      "title": "Episode Diagnostics",
      "type": "table"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },

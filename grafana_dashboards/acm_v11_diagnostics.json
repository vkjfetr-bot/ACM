                "id": "mappings",
                "value": [
                  { "options": { "DEGRADATION": { "color": "orange", "index": 0 } }, "type": "value" },
                  { "options": { "SPIKE": { "color": "red", "index": 1 } }, "type": "value" },
                  { "options": { "DRIFT": { "color": "yellow", "index": 2 } }, "type": "value" },
                  { "options": { "STUCK": { "color": "purple", "index": 3 } }, "type": "value" },
                  { "options": { "NOISE": { "color": "blue", "index": 4 } }, "type": "value" }

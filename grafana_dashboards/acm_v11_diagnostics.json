                "id": "mappings",
                "value": [
                  { "options": { "info": { "color": "blue", "index": 0 } }, "type": "value" },
                  { "options": { "warning": { "color": "yellow", "index": 1 } }, "type": "value" },
                  { "options": { "alert": { "color": "orange", "index": 2 } }, "type": "value" },
                  { "options": { "critical": { "color": "red", "index": 3 } }, "type": "value" }

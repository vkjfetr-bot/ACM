          "rawQuery": true,
          "rawSql": "SELECT \n  SensorName,\n  ContributionScore,\n  MeanZ,\n  MaxZ,\n  StdZ,\n  AnomalyCount\nFROM ACM_SensorHotspots\nWHERE EquipID = $equipment\nORDER BY ContributionScore DESC",
          "refId": "A"
        }

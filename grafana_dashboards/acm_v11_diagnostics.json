          "rawQuery": true,
          "rawSql": "SELECT SensorName, COUNT(*) AS Occurrences\nFROM ACM_EpisodeCulprits\nWHERE EquipID = $equipment\nGROUP BY SensorName\nORDER BY COUNT(*) DESC",
          "refId": "A"

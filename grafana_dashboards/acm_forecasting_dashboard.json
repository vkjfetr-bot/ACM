      "pluginVersion": "11.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT \n  Timestamp AS time,\n  OMR_Z AS value,\n  'OMR Score' AS metric\nFROM ACM_OMRTimeline\nWHERE EquipID = $equipment\n  AND Timestamp BETWEEN $__timeFrom() AND $__timeTo()\nORDER BY Timestamp ASC",
          "refId": "A"
        }
      ],
      "title": "OMR Score Timeline",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },

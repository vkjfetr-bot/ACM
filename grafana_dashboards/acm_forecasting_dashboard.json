      "id": 100,
      "panels": [],
      "title": "RUL & FAILURE PREDICTION",
      "type": "row"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Current Remaining Useful Life estimate with confidence bounds",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 24 },
              { "color": "yellow", "value": 72 },
              { "color": "green", "value": 168 }
            ]
          },
          "unit": "h"
        }
      },
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 1 },
      "id": 1,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "vertical",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "value_and_name"
      },
      "pluginVersion": "11.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT TOP 1 \n  ROUND(RUL_Hours, 1) AS 'RUL (Hours)'\nFROM ACM_RUL \nWHERE EquipID = $equipment \n  AND (P10_LowerBound IS NOT NULL OR P50_Median IS NOT NULL)\nORDER BY CreatedAt DESC",
          "refId": "A"
        }
      ],
      "title": "Current RUL",
      "type": "stat"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "P10 (pessimistic) to P90 (optimistic) RUL range",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "mappings": [],
          "unit": "h"
        }
      },
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 1 },
      "id": 2,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "horizontal",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "value_and_name"
      },
      "pluginVersion": "11.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT TOP 1 \n  ROUND(P10_LowerBound, 1) AS 'P10 (Pessimistic)',\n  ROUND(P50_Median, 1) AS 'P50 (Median)',\n  ROUND(P90_UpperBound, 1) AS 'P90 (Optimistic)'\nFROM ACM_RUL \nWHERE EquipID = $equipment \n  AND (P10_LowerBound IS NOT NULL OR P50_Median IS NOT NULL)\nORDER BY CreatedAt DESC",
          "refId": "A"
        }
      ],
      "title": "RUL Confidence Bounds",
      "type": "stat"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Forecast confidence and method used",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 0.5 },
              { "color": "yellow", "value": 0.7 },
              { "color": "green", "value": 0.85 }
            ]
          },
          "unit": "percentunit"
        }
      },
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 1 },
      "id": 3,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "vertical",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "value_and_name"
      },
      "pluginVersion": "11.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT TOP 1 \n  ROUND(Confidence, 2) AS 'Confidence',\n  Method AS 'Method'\nFROM ACM_RUL \nWHERE EquipID = $equipment \nORDER BY CreatedAt DESC",
          "refId": "A"
        }
      ],
      "title": "Forecast Confidence",
      "type": "stat"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Top sensors contributing to degradation",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "mappings": []
        }
      },
      "gridPos": { "h": 6, "w": 6, "x": 18, "y": 1 },
      "id": 4,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "horizontal",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "value_and_name"
      },
      "pluginVersion": "11.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT TOP 1 \n  COALESCE(TopSensor1, 'N/A') AS 'Top Sensor 1',\n  COALESCE(TopSensor2, 'N/A') AS 'Top Sensor 2',\n  COALESCE(TopSensor3, 'N/A') AS 'Top Sensor 3'\nFROM ACM_RUL \nWHERE EquipID = $equipment \nORDER BY CreatedAt DESC",
          "refId": "A"
        }
      ],
      "title": "Top Culprit Sensors",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 7 },
      "id": 101,
      "panels": [],
      "title": "HEALTH FORECAST TIMELINE",
      "type": "row"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Forecasted health trajectory with confidence intervals",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "Health %",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "insertNulls": false,
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "auto",
            "spanNulls": 3600000,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "dashed" }
          },
          "mappings": [],
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 30 },
              { "color": "yellow", "value": 60 },
              { "color": "green", "value": 80 }
            ]
          },
          "unit": "percent"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "CI_Lower" },
            "properties": [
              { "id": "custom.lineStyle", "value": { "dash": [10, 10], "fill": "dash" } },
              { "id": "custom.fillOpacity", "value": 0 },
              { "id": "color", "value": { "fixedColor": "light-blue", "mode": "fixed" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "CI_Upper" },
            "properties": [
              { "id": "custom.lineStyle", "value": { "dash": [10, 10], "fill": "dash" } },
              { "id": "custom.fillOpacity", "value": 0 },
              { "id": "color", "value": { "fixedColor": "light-blue", "mode": "fixed" } }
            ]
          }
        ]
      },
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 8 },
      "id": 5,
      "options": {
        "legend": { "calcs": ["lastNotNull", "min"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "maxHeight": 600, "mode": "multi", "sort": "desc" }
      },
      "pluginVersion": "11.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT \n  ForecastTimestamp AS time,\n  ForecastValue AS 'Health Forecast',\n  CiLower AS 'CI_Lower',\n  CiUpper AS 'CI_Upper'\nFROM ACM_HealthForecast_TS\nWHERE EquipID = $equipment\n  AND ForecastTimestamp >= DATEADD(DAY, -30, GETDATE())\nORDER BY ForecastTimestamp ASC",
          "refId": "A"
        }
      ],
      "title": "Health Forecast (7-Day Horizon)",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 18 },
      "id": 102,
      "panels": [],
      "title": "SENSOR FORECASTS",
      "type": "row"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Individual sensor Z-score forecasts",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": true,
            "axisColorMode": "text",
            "axisLabel": "Z-Score",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "insertNulls": false,
            "lineInterpolation": "smooth",
            "lineWidth": 1,
            "pointSize": 3,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "auto",
            "spanNulls": 3600000,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "off" }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 2 },
              { "color": "red", "value": 4 }
            ]
          }
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 19 },
      "id": 6,
      "options": {
        "legend": { "calcs": ["lastNotNull"], "displayMode": "table", "placement": "right", "showLegend": true },
        "tooltip": { "maxHeight": 600, "mode": "multi", "sort": "desc" }
      },
      "pluginVersion": "11.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT \n  Timestamp AS time,\n  ForecastValue AS value,\n  SensorName AS metric\nFROM ACM_SensorForecast\nWHERE EquipID = $equipment\n  AND Timestamp >= DATEADD(DAY, -14, GETDATE())\nORDER BY Timestamp ASC",
          "refId": "A"
        }
      ],
      "title": "Sensor Forecasts (Z-Score)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Multivariate VAR model sensor forecasts",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": true,
            "axisColorMode": "text",
            "axisLabel": "Z-Score",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "insertNulls": false,
            "lineInterpolation": "smooth",
            "lineWidth": 1,
            "pointSize": 3,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "auto",
            "spanNulls": 3600000,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "off" }
          },
          "mappings": []
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 19 },
      "id": 7,
      "options": {
        "legend": { "calcs": ["lastNotNull"], "displayMode": "table", "placement": "right", "showLegend": true },
        "tooltip": { "maxHeight": 600, "mode": "multi", "sort": "desc" }
      },
      "pluginVersion": "11.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT \n  ForecastTimestamp AS time,\n  ForecastValue AS value,\n  SensorName AS metric\nFROM ACM_MultivariateForecast\nWHERE EquipID = $equipment\n  AND ForecastTimestamp >= DATEADD(DAY, -14, GETDATE())\nORDER BY ForecastTimestamp ASC",
          "refId": "A"
        }
      ],
      "title": "Multivariate Forecasts (VAR Model)",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 27 },
      "id": 103,
      "panels": [],
      "title": "SENSOR CORRELATIONS",
      "type": "row"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Top correlated sensor pairs with lead/lag relationships",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "custom": {
            "align": "auto",
            "cellOptions": { "type": "auto" },
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "green", "value": 0.7 }
            ]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Correlation" },
            "properties": [
              { "id": "custom.cellOptions", "value": { "mode": "gradient", "type": "gauge", "valueDisplayMode": "text" } },
              { "id": "min", "value": -1 },
              { "id": "max", "value": 1 }
            ]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 28 },
      "id": 8,
      "options": {
        "cellHeight": "sm",
        "footer": { "countRows": false, "fields": "", "reducer": ["sum"], "show": false },
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "Correlation" }]
      },
      "pluginVersion": "11.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT TOP 20\n  SensorA,\n  SensorB,\n  ROUND(Correlation, 3) AS Correlation,\n  OptimalLag AS 'Lag (h)',\n  LeadSensor AS 'Lead Sensor',\n  CASE WHEN GrangerPValue < 0.05 THEN 'Yes' ELSE 'No' END AS 'Granger Causal'\nFROM ACM_SensorCorrelations\nWHERE EquipID = $equipment\nORDER BY ABS(Correlation) DESC",
          "refId": "A"
        }
      ],
      "title": "Top Sensor Correlations",
      "type": "table"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Sensors with significant Granger causality (one sensor predicts another)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "align": "auto",
            "cellOptions": { "type": "auto" },
            "inspect": false
          },
          "mappings": []
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 28 },
      "id": 9,
      "options": {
        "cellHeight": "sm",
        "footer": { "countRows": false, "fields": "", "reducer": ["sum"], "show": false },
        "showHeader": true
      },
      "pluginVersion": "11.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  LeadSensor AS 'Causing Sensor',\n  CASE WHEN LeadSensor = SensorA THEN SensorB ELSE SensorA END AS 'Affected Sensor',\n  ROUND(Correlation, 3) AS Correlation,\n  OptimalLag AS 'Lead Time (h)',\n  ROUND(GrangerPValue, 4) AS 'P-Value'\nFROM ACM_SensorCorrelations\nWHERE EquipID = $equipment\n  AND GrangerPValue < 0.05\n  AND LeadSensor IS NOT NULL\nORDER BY GrangerPValue ASC",
          "refId": "A"
        }
      ],
      "title": "Granger Causality (Lead-Lag Sensors)",
      "type": "table"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 36 },
      "id": 104,
      "panels": [],
      "title": "REGIME-CONDITIONED FORECASTING",
      "type": "row"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Per-regime degradation rates and RUL estimates",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "align": "auto",
            "cellOptions": { "type": "auto" },
            "inspect": false
          },
          "mappings": []
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 37 },
      "id": 10,
      "options": {
        "cellHeight": "sm",
        "footer": { "countRows": false, "fields": "", "reducer": ["sum"], "show": false },
        "showHeader": true
      },
      "pluginVersion": "11.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  RegimeLabel AS 'Regime',\n  ROUND(DegradationRate, 4) AS 'Degradation Rate',\n  ROUND(RUL_Hours, 1) AS 'RUL (h)',\n  ROUND(Confidence, 2) AS 'Confidence',\n  SampleCount AS 'Samples'\nFROM ACM_RUL_ByRegime\nWHERE EquipID = $equipment\nORDER BY RegimeLabel",
          "refId": "A"
        }
      ],
      "title": "RUL by Regime",
      "type": "table"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Current forecast context with OMR, drift, and regime state",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "mappings": []
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 37 },
      "id": 11,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "horizontal",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "value_and_name"
      },
      "pluginVersion": "11.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT TOP 1\n  CurrentRegime AS 'Current Regime',\n  ROUND(CurrentOmrZ, 2) AS 'OMR Z-Score',\n  ROUND(DriftScore, 2) AS 'Drift Score',\n  DriftTrend AS 'Drift Trend',\n  ROUND(ConfidenceMultiplier, 2) AS 'Confidence Adj'\nFROM ACM_ForecastContext\nWHERE EquipID = $equipment\nORDER BY CreatedAt DESC",
          "refId": "A"
        }
      ],
      "title": "Forecast Context",
      "type": "stat"
    }
  ],

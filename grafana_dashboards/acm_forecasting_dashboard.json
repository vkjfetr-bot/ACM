      },
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 1 },
      "id": 3,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "vertical",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "value_and_name"
      },
      "pluginVersion": "11.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT TOP 1 \n  ROUND(Confidence, 2) AS 'Confidence',\n  Method AS 'Method'\nFROM ACM_RUL \nWHERE EquipID = $equipment \nORDER BY CreatedAt DESC",
          "refId": "A"
        }
      ],
      "title": "Forecast Confidence",
      "type": "stat"
    },

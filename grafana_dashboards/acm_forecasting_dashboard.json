      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "custom": {
            "align": "auto",
            "cellOptions": { "type": "auto" },
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 3 }
            ]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Avg Contribution" },
            "properties": [
              { "id": "custom.cellOptions", "value": { "mode": "gradient", "type": "gauge", "valueDisplayMode": "text" } },
              { "id": "min", "value": 0 },
              { "id": "max", "value": 5 }
            ]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 19 },
      "id": 7,
      "options": {
        "cellHeight": "sm",
        "footer": { "countRows": false, "fields": "", "reducer": ["sum"], "show": false },
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "Avg Contribution" }]
      },
      "pluginVersion": "11.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT TOP 15\n  SensorName AS 'Sensor',\n  ROUND(AVG(ContributionScore), 3) AS 'Avg Contribution',\n  ROUND(MAX(ContributionScore), 3) AS 'Max Contribution',\n  COUNT(*) AS 'Occurrences'\nFROM ACM_OMRContributionsLong\nWHERE EquipID = $equipment\n  AND Timestamp BETWEEN $__timeFrom() AND $__timeTo()\nGROUP BY SensorName\nORDER BY AVG(ContributionScore) DESC",
          "refId": "A"
        }
      ],
      "title": "Top OMR Contributors",
      "type": "table"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 27 },
      "id": 103,

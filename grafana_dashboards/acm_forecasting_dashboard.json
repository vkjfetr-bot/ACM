    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Current Remaining Useful Life estimate with confidence bounds",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 24 },
              { "color": "yellow", "value": 72 },
              { "color": "green", "value": 168 }
            ]
          },
          "unit": "h"
        }
      },
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 1 },
      "id": 1,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "vertical",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "value_and_name"
      },
      "pluginVersion": "11.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT TOP 1 \n  ROUND(RUL_Hours, 1) AS 'RUL (Hours)'\nFROM ACM_RUL \nWHERE EquipID = $equipment \n  AND (P10_LowerBound IS NOT NULL OR P50_Median IS NOT NULL)\nORDER BY CreatedAt DESC",
          "refId": "A"
        }
      ],
      "title": "Current RUL",
      "type": "stat"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "P10 (pessimistic) to P90 (optimistic) RUL range",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "mappings": [],
          "unit": "h"
        }
      },
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 1 },
      "id": 2,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "horizontal",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "value_and_name"
      },
      "pluginVersion": "11.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT TOP 1 \n  ROUND(P10_LowerBound, 1) AS 'P10 (Pessimistic)',\n  ROUND(P50_Median, 1) AS 'P50 (Median)',\n  ROUND(P90_UpperBound, 1) AS 'P90 (Optimistic)'\nFROM ACM_RUL \nWHERE EquipID = $equipment \n  AND (P10_LowerBound IS NOT NULL OR P50_Median IS NOT NULL)\nORDER BY CreatedAt DESC",
          "refId": "A"
        }
      ],
      "title": "RUL Confidence Bounds",
      "type": "stat"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },

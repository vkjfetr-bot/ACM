      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "#73BF69", "value": null },
              { "color": "#FF9830", "value": 1 },
              { "color": "#C4162A", "value": 5 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 },
      "id": 4,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "horizontal",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showPercentChange": false,
        "textMode": "value_and_name",
        "wideLayout": true
      },
      "pluginVersion": "12.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT COUNT(*) AS 'Errors'\nFROM ACM_Runs\nWHERE $__timeFilter(CreatedAt)\n  AND (Status = 'ERROR' OR Status = 'FAILED')",
          "refId": "A"
        }
      ],
      "title": "ERRORS",
      "type": "stat"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Average run duration",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "decimals": 1,
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "#73BF69", "value": null },
              { "color": "#FADE2A", "value": 120 },
              { "color": "#C4162A", "value": 300 }
            ]
          },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "id": 5,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "horizontal",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showPercentChange": false,
        "textMode": "value_and_name",
        "wideLayout": true
      },
      "pluginVersion": "12.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT AVG(CAST(DurationSeconds AS FLOAT)) AS 'Avg Duration'\nFROM ACM_Runs\nWHERE $__timeFilter(CreatedAt)\n  AND DurationSeconds IS NOT NULL",
          "refId": "A"
        }
      ],
      "title": "AVG DURATION",
      "type": "stat"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Number of equipment being monitored",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "#8AB8FF", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 },
      "id": 6,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "horizontal",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showPercentChange": false,
        "textMode": "value_and_name",
        "wideLayout": true
      },
      "pluginVersion": "12.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT COUNT(DISTINCT EquipID) AS 'Equipment'\nFROM ACM_Runs\nWHERE $__timeFilter(CreatedAt)",
          "refId": "A"
        }
      ],
      "title": "EQUIPMENT",
      "type": "stat"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Data points processed",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "#B877D9", "value": null }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 },
      "id": 7,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "horizontal",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showPercentChange": false,
        "textMode": "value_and_name",
        "wideLayout": true
      },
      "pluginVersion": "12.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT COALESCE(SUM(RowsProcessed), 0) AS 'Rows Processed'\nFROM ACM_Runs\nWHERE $__timeFilter(CreatedAt)",
          "refId": "A"
        }
      ],
      "title": "DATA PROCESSED",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "id": 10,
      "panels": [],
      "title": "RUN PERFORMANCE",
      "type": "row"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Run duration over time by equipment",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "Duration (s)",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "barWidthFactor": 0.8,
            "drawStyle": "bars",
            "fillOpacity": 70,
            "gradientMode": "hue",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "insertNulls": false,
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "never",
            "spanNulls": 3600000,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "line+area" }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 120 },
              { "color": "red", "value": 300 }
            ]
          },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 10, "w": 16, "x": 0, "y": 6 },
      "id": 11,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "right", "showLegend": true },
        "tooltip": { "hideZeros": false, "mode": "multi", "sort": "desc" }
      },
      "pluginVersion": "12.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT r.CreatedAt AS time, r.DurationSeconds AS value, e.EquipName AS metric\nFROM ACM_Runs r\nJOIN Equipment e ON r.EquipID = e.EquipID\nWHERE $__timeFilter(r.CreatedAt)\n  AND r.DurationSeconds IS NOT NULL\n  AND ($equipment = 0 OR r.EquipID = $equipment)\nORDER BY r.CreatedAt ASC",
          "refId": "A"
        }
      ],
      "title": "RUN DURATION OVER TIME",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Run statistics by equipment",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "custom": { "align": "center", "cellOptions": { "type": "auto" }, "filterable": true, "inspect": false },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "text", "value": null }]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Avg (s)" },
            "properties": [
              { "id": "custom.cellOptions", "value": { "mode": "gradient", "type": "gauge", "valueDisplayMode": "text" } },
              { "id": "max", "value": 300 }
            ]
          }
        ]
      },
      "gridPos": { "h": 10, "w": 8, "x": 16, "y": 6 },
      "id": 12,
      "options": {
        "cellHeight": "sm",
        "footer": { "countRows": false, "enablePagination": false, "fields": "", "reducer": ["sum"], "show": false },
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "Runs" }]
      },
      "pluginVersion": "12.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  e.EquipName AS 'Equipment',\n  COUNT(*) AS 'Runs',\n  ROUND(AVG(CAST(r.DurationSeconds AS FLOAT)), 1) AS 'Avg (s)',\n  MAX(r.DurationSeconds) AS 'Max (s)',\n  SUM(CASE WHEN r.Status = 'SUCCESS' THEN 1 ELSE 0 END) AS 'Success',\n  SUM(CASE WHEN r.Status IN ('ERROR', 'FAILED') THEN 1 ELSE 0 END) AS 'Errors'\nFROM ACM_Runs r\nJOIN Equipment e ON r.EquipID = e.EquipID\nWHERE $__timeFilter(r.CreatedAt)\n  AND r.DurationSeconds IS NOT NULL\n  AND ($equipment = 0 OR r.EquipID = $equipment)\nGROUP BY e.EquipName\nORDER BY COUNT(*) DESC",
          "refId": "A"
        }
      ],
      "title": "EQUIPMENT STATISTICS",
      "type": "table"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 16 },
      "id": 20,
      "panels": [],
      "title": "EXECUTION BREAKDOWN",
      "type": "row"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Time spent in each pipeline section",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "fillOpacity": 80,
            "gradientMode": "hue",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "lineWidth": 1,
            "scaleDistribution": { "type": "linear" },
            "thresholdsStyle": { "mode": "off" }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 17 },
      "id": 21,
      "options": {
        "barRadius": 0.1,
        "barWidth": 0.8,
        "colorByField": "AvgDuration",
        "fullHighlight": false,
        "groupWidth": 0.7,
        "legend": { "calcs": [], "displayMode": "list", "placement": "bottom", "showLegend": false },
        "orientation": "horizontal",
        "showValue": "auto",
        "stacking": "none",
        "tooltip": { "hideZeros": false, "mode": "single", "sort": "none" },
        "xTickLabelRotation": 0,
        "xTickLabelSpacing": 0
      },
      "pluginVersion": "12.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT Section, AVG(DurationSeconds) AS AvgDuration\nFROM ACM_RunTimers\nWHERE $__timeFilter(CreatedAt)\n  AND ($equipment = 0 OR EquipID = $equipment)\nGROUP BY Section\nORDER BY AvgDuration DESC",
          "refId": "A"
        }
      ],
      "title": "AVERAGE TIME BY SECTION",
      "type": "barchart"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Execution time breakdown over time (stacked)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "fillOpacity": 80,
            "gradientMode": "hue",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "lineWidth": 1,
            "scaleDistribution": { "type": "linear" },
            "thresholdsStyle": { "mode": "off" }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 10, "w": 12, "x": 12, "y": 17 },
      "id": 22,
      "options": {
        "barRadius": 0,
        "barWidth": 0.9,
        "fullHighlight": false,
        "groupWidth": 0.7,
        "legend": { "calcs": ["lastNotNull", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "orientation": "auto",
        "showValue": "never",
        "stacking": "normal",
        "tooltip": { "hideZeros": true, "mode": "multi", "sort": "desc" },
        "xTickLabelRotation": 0,
        "xTickLabelSpacing": 100
      },
      "pluginVersion": "12.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT CreatedAt AS time, DurationSeconds AS value, Section AS metric\nFROM ACM_RunTimers\nWHERE $__timeFilter(CreatedAt)\n  AND ($equipment = 0 OR EquipID = $equipment)\nORDER BY CreatedAt ASC",
          "refId": "A"
        }
      ],
      "title": "SECTION TIMING OVER TIME",
      "type": "barchart"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 27 },
      "id": 30,
      "panels": [],
      "title": "LOGS & ERRORS",
      "type": "row"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Recent error and warning logs",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "custom": { "align": "auto", "cellOptions": { "type": "auto" }, "filterable": true, "inspect": true },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "text", "value": null }]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Level" },
            "properties": [
              { "id": "mappings", "value": [
                { "options": { "ERROR": { "color": "#C4162A", "index": 0 } }, "type": "value" },
                { "options": { "WARNING": { "color": "#FF9830", "index": 1 } }, "type": "value" },
                { "options": { "INFO": { "color": "#73BF69", "index": 2 } }, "type": "value" }
              ]},
              { "id": "custom.cellOptions", "value": { "mode": "gradient", "type": "color-background" } },
              { "id": "custom.width", "value": 80 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Message" },
            "properties": [{ "id": "custom.width", "value": 600 }]
          },
          {
            "matcher": { "id": "byName", "options": "Time" },
            "properties": [{ "id": "custom.width", "value": 180 }]
          }
        ]
      },
      "gridPos": { "h": 12, "w": 24, "x": 0, "y": 28 },
      "id": 31,
      "options": {
        "cellHeight": "sm",
        "footer": { "countRows": false, "enablePagination": false, "fields": "", "reducer": ["sum"], "show": false },
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "Time" }]
      },
      "pluginVersion": "12.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT TOP 100\n  FORMAT(LoggedAt, 'yyyy-MM-dd HH:mm:ss') AS 'Time',\n  Level,\n  e.EquipName AS 'Equipment',\n  Message\nFROM ACM_RunLogs l\nLEFT JOIN Equipment e ON l.EquipID = e.EquipID\nWHERE $__timeFilter(l.LoggedAt)\n  AND l.Level IN ('ERROR', 'WARNING')\n  AND ($equipment = 0 OR l.EquipID = $equipment)\nORDER BY l.LoggedAt DESC",
          "refId": "A"
        }
      ],
      "title": "RECENT ERRORS & WARNINGS",
      "type": "table"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 40 },
      "id": 40,
      "panels": [],
      "title": "DATA QUALITY",
      "type": "row"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Model refit requests over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "Count",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "barWidthFactor": 0.6,
            "drawStyle": "bars",
            "fillOpacity": 80,
            "gradientMode": "none",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "insertNulls": false,
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "off" }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 41 },
      "id": 41,
      "options": {
        "legend": { "calcs": ["sum"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "hideZeros": false, "mode": "multi", "sort": "desc" }
      },
      "pluginVersion": "12.0.0",
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT \n  DATEADD(HOUR, DATEDIFF(HOUR, 0, RequestedAt), 0) AS time,\n  COUNT(*) AS value,\n  'Refit Requests' AS metric\nFROM ACM_RefitRequests\nWHERE $__timeFilter(RequestedAt)\n  AND ($equipment = 0 OR EquipID = $equipment)\nGROUP BY DATEADD(HOUR, DATEDIFF(HOUR, 0, RequestedAt), 0)\nORDER BY time ASC",
          "refId": "A"

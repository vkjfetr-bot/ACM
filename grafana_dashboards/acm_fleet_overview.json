      "targets": [
        {
          "datasource": {
            "type": "mssql",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT \n    h.Timestamp AS time,\n    e.EquipName AS metric,\n    h.HealthIndex AS value\nFROM ACM_HealthTimeline h\nJOIN Equipment e ON h.EquipID = e.EquipID\nWHERE h.Timestamp BETWEEN $__timeFrom() AND $__timeTo()\n    AND e.IsActive = 1\nUNION ALL\nSELECT \n    h.Timestamp AS time,\n    'Fleet Average' AS metric,\n    AVG(h.HealthIndex) AS value\nFROM ACM_HealthTimeline h\nJOIN Equipment e ON h.EquipID = e.EquipID\nWHERE h.Timestamp BETWEEN $__timeFrom() AND $__timeTo()\n    AND e.IsActive = 1\nGROUP BY h.Timestamp\nORDER BY time ASC",
          "refId": "A"
        }
      ],
      "title": "Fleet Health Timeline",
      "type": "timeseries"

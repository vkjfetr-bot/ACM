        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH LatestHealth AS (\n  SELECT h.EquipID, h.Health, h.HealthZone, h.FusedZ, h.Timestamp,\n    ROW_NUMBER() OVER (PARTITION BY h.EquipID ORDER BY h.Timestamp DESC) AS rn\n  FROM ACM_HealthTimeline h\n),\nLatestRUL AS (\n  SELECT r.EquipID, r.RUL_Hours, r.Confidence,\n    ROW_NUMBER() OVER (PARTITION BY r.EquipID ORDER BY r.CreatedAt DESC) AS rn\n  FROM ACM_RUL r\n  WHERE r.P10_LowerBound IS NOT NULL OR r.P50_Median IS NOT NULL\n)\nSELECT\n  e.EquipCode AS 'Equipment',\n  e.EquipName AS 'Name',\n  ROUND(lh.Health, 1) AS 'Health',\n  lh.HealthZone AS 'Zone',\n  ROUND(lh.FusedZ, 2) AS 'FusedZ',\n  ROUND(lr.RUL_Hours, 1) AS 'RUL (h)',\n  ROUND(lr.Confidence, 2) AS 'Conf',\n  FORMAT(lh.Timestamp, 'yyyy-MM-dd HH:mm') AS 'Last Update'\nFROM Equipment e\nLEFT JOIN LatestHealth lh ON e.EquipID = lh.EquipID AND lh.rn = 1\nLEFT JOIN LatestRUL lr ON e.EquipID = lr.EquipID AND lr.rn = 1\nWHERE lh.Health IS NOT NULL\nORDER BY lh.Health ASC",
          "refId": "A"
        }
      ],
      "title": "Equipment Status",
      "type": "table"

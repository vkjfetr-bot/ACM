        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT\n  DATEADD(DAY, DATEDIFF(DAY, 0, PeriodStart), 0) AS time,\n  SUM(CASE WHEN HealthZone = 'GOOD' THEN EquipmentCount ELSE 0 END) AS 'GOOD',\n  SUM(CASE WHEN HealthZone = 'WATCH' THEN EquipmentCount ELSE 0 END) AS 'WATCH',\n  SUM(CASE WHEN HealthZone = 'ALERT' THEN EquipmentCount ELSE 0 END) AS 'ALERT',\n  SUM(CASE WHEN HealthZone = 'CRITICAL' THEN EquipmentCount ELSE 0 END) AS 'CRITICAL'\nFROM ACM_HealthZoneByPeriod\nWHERE PeriodStart BETWEEN $__timeFrom() AND $__timeTo()\nGROUP BY DATEADD(DAY, DATEDIFF(DAY, 0, PeriodStart), 0)\nORDER BY time ASC",
          "refId": "A"
        }

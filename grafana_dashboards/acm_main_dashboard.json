      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT \n    Timestamp AS time,\n    ar1_z AS 'AR1',\n    pca_spe_z AS 'PCA-SPE',\n    pca_t2_z AS 'PCA-T2',\n    iforest_z AS 'IForest',\n    gmm_z AS 'GMM',\n    fused AS 'Fused'\nFROM ACM_Scores_Wide\nWHERE EquipID = $equipment\n  AND Timestamp BETWEEN $__timeFrom() AND $__timeTo()\nORDER BY time ASC",
          "refId": "A"
        }
      ],
      "title": "Detector Signals Timeline",
      "type": "timeseries"
    },
    {

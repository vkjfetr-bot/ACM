        "cellHeight": "sm",
        "footer": { "countRows": false, "fields": "", "reducer": ["sum"], "show": false },
        "showHeader": true
      },
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT TOP 10\n    SensorName,\n    DetectorType,\n    ROUND(ContributionPct, 1) AS ContributionPct,\n    Rank\nFROM ACM_EpisodeCulprits\nWHERE EquipID = $equipment\nORDER BY CreatedAt DESC, Rank ASC",
          "refId": "A"
        }
      ],
      "title": "Episode Culprit Sensors",
      "type": "table"

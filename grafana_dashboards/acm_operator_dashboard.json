      "type": "stat",
      "title": "Worst Sensor (Current Run)",
      "gridPos": { "h": 3, "w": 4, "x": 20, "y": 0 },
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [

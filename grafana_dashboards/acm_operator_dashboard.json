        "showPercentChange": false,
        "text": {},
        "textMode": "auto",
        "wideLayout": true
      },
      "pluginVersion": "12.2.1",
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT TOP 1\n  CASE\n    WHEN HealthIndex >= 85 THEN 'HEALTHY'\n    WHEN HealthIndex >= 70 THEN 'CAUTION'\n    ELSE 'ALERT'\n  END AS Status\nFROM ACM_HealthTimeline\nWHERE EquipID = $equipment\nORDER BY Timestamp DESC"
        }

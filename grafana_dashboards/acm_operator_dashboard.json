      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  DATEDIFF(DAY, MIN(StartTimestamp), SYSUTCDATETIME()) AS DaysSinceLastAlert\nFROM ACM_AlertAge\nWHERE EquipID = $equipment"

          "rawQuery": true,
          "rawSql": "SELECT\n  EarliestMaintenance,\n  PreferredWindowStart,\n  PreferredWindowEnd,\n  FailureProbAtWindowEnd * 100.0 AS FailureProbabilityAtEnd\nFROM ACM_MaintenanceRecommendation\nWHERE EquipID = $equipment\n  AND RunID = (\n      SELECT TOP 1 RunID\n      FROM ACM_MaintenanceRecommendation\n      WHERE EquipID = $equipment\n      ORDER BY PreferredWindowEnd DESC\n  )"
        }
      ]
    }

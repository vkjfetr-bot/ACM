          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "/^CurrentZ$/",
          "values": false
        },
        "showPercentChange": false,
        "textMode": "value_and_name"
      },
      "pluginVersion": "12.2.1",
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT TOP 1\n  SensorName AS WorstSensor,\n  ROUND(LatestAbsZ, 2) AS CurrentZ\nFROM ACM_SensorHotspots_Latest\nWHERE EquipID = $equipment\n  AND RunID = (\n      SELECT TOP 1 RunID\n      FROM ACM_SensorHotspots_Latest\n      WHERE EquipID = $equipment\n      ORDER BY RunID DESC\n  )\nORDER BY LatestAbsZ DESC"
        }
      ]
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Health Index Timeline",
      "gridPos": { "h": 7, "w": 16, "x": 0, "y": 3 },
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "fieldConfig": {

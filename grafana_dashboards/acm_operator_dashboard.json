          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT\n  Timestamp AS time,\n  ForecastHealth AS [Forecast Health],\n  CiLower AS [CI Lower],\n  CiUpper AS [CI Upper]\nFROM ACM_HealthForecast_TS\nWHERE EquipID = $equipment\n  AND RunID = (\n      SELECT TOP 1 RunID\n      FROM ACM_HealthForecast_TS\n      WHERE EquipID = $equipment\n      ORDER BY RunID DESC\n  )\n  AND $__timeFilter(Timestamp)\nORDER BY Timestamp"
        }
      ]
    },
    {
      "id": 11,
      "type": "table",
      "title": "Sensor Hotspots",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 10 },
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "custom": {
            "align": "auto",
            "cellOptions": { "type": "auto" },
            "footer": { "reducers": [] },
            "inspect": false
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 2 },
              { "color": "orange", "value": 2.5 },
              { "color": "red", "value": 3 }
            ]

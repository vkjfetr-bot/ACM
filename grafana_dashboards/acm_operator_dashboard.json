        "overrides": []
      },
      "options": {
        "cellHeight": "sm",
        "showHeader": true,
        "sortBy": [
          { "desc": true, "displayName": "Current Z-Score" }
        ]
      },
      "pluginVersion": "12.2.1",
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT TOP 20\n       SensorName AS [Sensor Name],\n       ROUND(LatestAbsZ, 2) AS [Current Z-Score],\n       ROUND(MaxAbsZ, 2)    AS [Peak Z-Score],\n       MaxTimestamp          AS [Peak Time],\n       ROUND(LatestValue, 2) AS [Current Value],\n       ROUND(TrainMean, 2)   AS [Normal Mean],\n       AboveAlertCount       AS [Alert Count]\nFROM ACM_SensorHotspots_Latest\nWHERE EquipID = $equipment\n  AND RunID = (\n      SELECT TOP 1 RunID\n      FROM ACM_SensorHotspots_Latest\n      WHERE EquipID = $equipment\n      ORDER BY RunID DESC\n  )\nORDER BY [Current Z-Score] DESC"
        }
      ]
    },
    {
      "id": 24,
      "type": "table",
      "title": "Failure Attribution by Sensor (at Predicted Failure Time)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 10 },
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "custom": {
            "align": "auto",
            "cellOptions": { "type": "auto" },
            "footer": { "reducers": [] },
            "inspect": false
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 0.2 },
              { "color": "red", "value": 0.5 }
            ]
          },
          "unit": "percent"
        },
        "overrides": []
      },
      "options": {
        "cellHeight": "sm",
        "showHeader": true,
        "sortBy": [
          { "desc": true, "displayName": "FailureContribution" }
        ]
      },
      "pluginVersion": "12.2.1",
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT TOP 10\n  ra.SensorName,\n  ra.FailureContribution * 100.0 AS FailureContribution,\n  ra.ZScoreAtFailure,\n  ra.AlertCount,\n  ra.FailureTime,\n  ISNULL(ff.FailureProb * 100.0, 0.0) AS FailureProbabilityAtTime\nFROM ACM_RUL_Attribution ra\nOUTER APPLY (\n  SELECT TOP 1 FailureProb\n  FROM ACM_FailureForecast_TS ff\n  WHERE ff.EquipID = ra.EquipID\n    AND ff.RunID = ra.RunID\n    AND ff.Timestamp >= ra.FailureTime\n  ORDER BY ff.Timestamp ASC\n) ff\nWHERE ra.EquipID = $equipment\n  AND ra.RunID = (\n      SELECT TOP 1 RunID\n      FROM ACM_RUL_Attribution\n      WHERE EquipID = $equipment\n      ORDER BY FailureTime DESC\n  )\nORDER BY FailureContribution DESC"
        }
      ]
    },

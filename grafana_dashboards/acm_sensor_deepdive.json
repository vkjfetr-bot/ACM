        {
          "datasource": {
            "type": "mssql",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "-- Pivot detector signals by sensor\nSELECT \n    SensorName AS 'Sensor',\n    MAX(CASE WHEN DetectorLabel LIKE '%AR1%' THEN ROUND(CurrentZ, 2) ELSE NULL END) AS 'AR1',\n    MAX(CASE WHEN DetectorLabel LIKE '%PCA-SPE%' THEN ROUND(CurrentZ, 2) ELSE NULL END) AS 'PCA-SPE',\n    MAX(CASE WHEN DetectorLabel LIKE '%PCA-T%' THEN ROUND(CurrentZ, 2) ELSE NULL END) AS 'PCA-T\u00b2',\n    MAX(CASE WHEN DetectorLabel LIKE '%IForest%' OR DetectorLabel LIKE '%Isolation%' THEN ROUND(CurrentZ, 2) ELSE NULL END) AS 'IForest',\n    MAX(CASE WHEN DetectorLabel LIKE '%GMM%' OR DetectorLabel LIKE '%Gaussian%' THEN ROUND(CurrentZ, 2) ELSE NULL END) AS 'GMM',\n    MAX(CASE WHEN DetectorLabel LIKE '%OMR%' OR DetectorLabel LIKE '%Overall%' THEN ROUND(CurrentZ, 2) ELSE NULL END) AS 'OMR'\nFROM ACM_SensorDefects\nWHERE EquipID = $equipment\n    AND ActiveDefect = 1\nGROUP BY SensorName\nORDER BY \n    COALESCE(MAX(CASE WHEN DetectorLabel LIKE '%AR1%' THEN CurrentZ ELSE 0 END), 0) +\n    COALESCE(MAX(CASE WHEN DetectorLabel LIKE '%PCA%' THEN CurrentZ ELSE 0 END), 0) +\n    COALESCE(MAX(CASE WHEN DetectorLabel LIKE '%IForest%' THEN CurrentZ ELSE 0 END), 0) +\n    COALESCE(MAX(CASE WHEN DetectorLabel LIKE '%GMM%' THEN CurrentZ ELSE 0 END), 0) +\n    COALESCE(MAX(CASE WHEN DetectorLabel LIKE '%OMR%' THEN CurrentZ ELSE 0 END), 0) DESC",
          "refId": "A"
        }
      ],
      "title": "Detector Signals Heatmap (By Sensor)",
      "type": "table"
    },

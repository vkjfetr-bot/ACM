{
  "annotations": {
    "list": []
  },
  "description": "ACM V11 Dashboard - Confidence, Maturity, RUL Reliability, UNKNOWN Regimes",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "panels": [],
      "title": "V11 MODEL STATE",
      "type": "row"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Current model maturity state for selected equipment",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [
            {
              "options": {
                "COLDSTART": { "color": "blue", "index": 0, "text": "COLDSTART" },
                "LEARNING": { "color": "yellow", "index": 1, "text": "LEARNING" },
                "CONVERGED": { "color": "green", "index": 2, "text": "CONVERGED" },
                "DEPRECATED": { "color": "dark-red", "index": 3, "text": "DEPRECATED" }
              },
              "type": "value"
            }
          ],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "gray", "value": null }] }
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "id": 2,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "/^MaturityState$/", "values": false },
        "textMode": "value"
      },
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT TOP 1 MaturityState FROM ACM_RUL WHERE EquipID = $equipment ORDER BY CreatedAt DESC",
          "refId": "A"
        }
      ],
      "title": "Model Maturity",
      "type": "stat"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Current RUL reliability status - shows if prediction is trustworthy",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [
            {
              "options": {
                "RELIABLE": { "color": "green", "index": 0, "text": "RELIABLE" },
                "NOT_RELIABLE": { "color": "red", "index": 1, "text": "NOT RELIABLE" },
                "LEARNING": { "color": "yellow", "index": 2, "text": "LEARNING" },
                "INSUFFICIENT_DATA": { "color": "orange", "index": 3, "text": "INSUFFICIENT DATA" }
              },
              "type": "value"
            }
          ],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "gray", "value": null }] }
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 1 },
      "id": 3,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "/^RUL_Status$/", "values": false },
        "textMode": "value"
      },
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT TOP 1 RUL_Status FROM ACM_RUL WHERE EquipID = $equipment ORDER BY CreatedAt DESC",
          "refId": "A"
        }
      ],
      "title": "RUL Reliability",
      "type": "stat"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Overall confidence in current predictions (0-1)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "max": 1,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 0.3 },
              { "color": "yellow", "value": 0.5 },
              { "color": "green", "value": 0.7 }
            ]
          },
          "unit": "percentunit"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "id": 4,
      "options": {
        "minVizHeight": 75,
        "minVizWidth": 75,
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showThresholdLabels": false,
        "showThresholdMarkers": true,
        "sizing": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT TOP 1 Confidence FROM ACM_RUL WHERE EquipID = $equipment ORDER BY CreatedAt DESC",
          "refId": "A"
        }
      ],
      "title": "RUL Confidence",
      "type": "gauge"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Remaining useful life in hours (median prediction)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 72 },
              { "color": "yellow", "value": 168 },
              { "color": "green", "value": 720 }
            ]
          },
          "unit": "h"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 },
      "id": 5,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT TOP 1 ROUND(P50_Median, 1) AS RUL FROM ACM_RUL WHERE EquipID = $equipment AND P50_Median IS NOT NULL ORDER BY CreatedAt DESC",
          "refId": "A"
        }
      ],
      "title": "RUL (Median)",
      "type": "stat"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Current health percentage",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "max": 100,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 30 },
              { "color": "yellow", "value": 60 },
              { "color": "green", "value": 80 }
            ]
          },
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 },
      "id": 6,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT TOP 1 ROUND(HealthPercent, 1) AS Health FROM ACM_HealthTimeline WHERE EquipID = $equipment ORDER BY Timestamp DESC",
          "refId": "A"
        }
      ],
      "title": "Health %",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "id": 10,
      "panels": [],
      "title": "RUL PREDICTION DETAILS",
      "type": "row"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Full RUL prediction with confidence intervals and reliability status",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "custom": { "align": "auto", "cellOptions": { "type": "auto" }, "inspect": false },
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "RUL_Status" },
            "properties": [
              {
                "id": "mappings",
                "value": [
                  {
                    "options": {
                      "RELIABLE": { "color": "green", "index": 0, "text": "RELIABLE" },
                      "NOT_RELIABLE": { "color": "red", "index": 1, "text": "NOT RELIABLE" },
                      "LEARNING": { "color": "yellow", "index": 2, "text": "LEARNING" },
                      "INSUFFICIENT_DATA": { "color": "orange", "index": 3, "text": "INSUFFICIENT DATA" }
                    },
                    "type": "value"
                  }
                ]
              },
              { "id": "custom.cellOptions", "value": { "type": "color-background" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "MaturityState" },
            "properties": [
              {
                "id": "mappings",
                "value": [
                  {
                    "options": {
                      "CONVERGED": { "color": "green", "index": 0, "text": "CONVERGED" },
                      "LEARNING": { "color": "yellow", "index": 1, "text": "LEARNING" },
                      "COLDSTART": { "color": "blue", "index": 2, "text": "COLDSTART" },
                      "DEPRECATED": { "color": "dark-red", "index": 3, "text": "DEPRECATED" }
                    },
                    "type": "value"
                  }
                ]
              },
              { "id": "custom.cellOptions", "value": { "type": "color-background" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "RUL_Hours" },
            "properties": [
              { "id": "unit", "value": "h" },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    { "color": "red", "value": null },
                    { "color": "orange", "value": 72 },
                    { "color": "yellow", "value": 168 },
                    { "color": "green", "value": 720 }
                  ]
                }
              },
              { "id": "custom.cellOptions", "value": { "type": "color-background" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Confidence" },
            "properties": [
              { "id": "unit", "value": "percentunit" },
              { "id": "custom.cellOptions", "value": { "mode": "gradient", "type": "gauge" } },
              { "id": "min", "value": 0 },
              { "id": "max", "value": 1 }
            ]
          }
        ]
      },
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 6 },
      "id": 11,
      "options": { "cellHeight": "sm", "footer": { "countRows": false, "fields": "", "reducer": ["sum"], "show": false }, "showHeader": true },
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT TOP 1\n    RUL_Status,\n    MaturityState,\n    ROUND(RUL_Hours, 1) AS RUL_Hours,\n    ROUND(P10_LowerBound, 1) AS P10,\n    ROUND(P50_Median, 1) AS P50,\n    ROUND(P90_UpperBound, 1) AS P90,\n    ROUND(Confidence, 3) AS Confidence,\n    Method,\n    TopSensor1,\n    TopSensor2,\n    TopSensor3,\n    CreatedAt\nFROM ACM_RUL\nWHERE EquipID = $equipment\nORDER BY CreatedAt DESC",
          "refId": "A"
        }
      ],
      "title": "Latest RUL Prediction",
      "type": "table"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 12 },
      "id": 20,
      "panels": [],
      "title": "CONFIDENCE TRENDS",
      "type": "row"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "RUL confidence over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "Confidence",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 30,
            "gradientMode": "opacity",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "insertNulls": false,
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "auto",
            "spanNulls": 3600000,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "line" }
          },
          "mappings": [],
          "max": 1,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "green", "value": 0.7 }
            ]
          },
          "unit": "percentunit"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 13 },
      "id": 21,
      "options": {
        "legend": { "calcs": ["mean", "lastNotNull"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT\n    CreatedAt AS time,\n    Confidence AS confidence\nFROM ACM_RUL\nWHERE EquipID = $equipment\n  AND CreatedAt BETWEEN $__timeFrom() AND $__timeTo()\nORDER BY time ASC",
          "refId": "A"
        }
      ],
      "title": "RUL Confidence Over Time",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Health confidence over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "Confidence",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 30,
            "gradientMode": "opacity",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "insertNulls": false,
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "auto",
            "spanNulls": 3600000,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "line" }
          },
          "mappings": [],
          "max": 1,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "green", "value": 0.7 }
            ]
          },
          "unit": "percentunit"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 13 },
      "id": 22,
      "options": {
        "legend": { "calcs": ["mean", "lastNotNull"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT\n    Timestamp AS time,\n    Confidence AS confidence\nFROM ACM_HealthTimeline\nWHERE EquipID = $equipment\n  AND Timestamp BETWEEN $__timeFrom() AND $__timeTo()\n  AND Confidence IS NOT NULL\nORDER BY time ASC",
          "refId": "A"
        }
      ],
      "title": "Health Confidence Over Time",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 21 },
      "id": 30,
      "panels": [],
      "title": "REGIME ANALYSIS",
      "type": "row"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Operating regime over time. UNKNOWN (-1, dark purple) indicates low confidence assignment.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "custom": { "fillOpacity": 70, "insertNulls": false, "lineWidth": 0, "spanNulls": false },
          "mappings": [
            {
              "options": { "-1": { "color": "dark-purple", "index": 0, "text": "UNKNOWN" } },
              "type": "value"
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "dark-purple", "value": null },
              { "color": "blue", "value": 0 },
              { "color": "green", "value": 1 },
              { "color": "yellow", "value": 2 },
              { "color": "orange", "value": 3 },
              { "color": "red", "value": 4 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 5, "w": 24, "x": 0, "y": 22 },
      "id": 31,
      "options": {
        "alignValue": "center",
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "mergeValues": true,
        "rowHeight": 0.9,
        "showValue": "auto",
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT\n    Timestamp AS time,\n    RegimeLabel AS Regime\nFROM ACM_RegimeTimeline\nWHERE EquipID = $equipment\n  AND Timestamp BETWEEN $__timeFrom() AND $__timeTo()\nORDER BY time ASC",
          "refId": "A"
        }
      ],
      "title": "Operating Regime Timeline",
      "type": "state-timeline"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Regime assignment confidence over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "Confidence",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "insertNulls": false,
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "never",
            "spanNulls": 3600000,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "line" }
          },
          "mappings": [],
          "max": 1,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "green", "value": 0.7 }
            ]
          },
          "unit": "percentunit"
        },
        "overrides": []
      },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 27 },
      "id": 32,
      "options": {
        "legend": { "calcs": ["mean", "min"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT\n    Timestamp AS time,\n    AssignmentConfidence AS confidence\nFROM ACM_RegimeTimeline\nWHERE EquipID = $equipment\n  AND Timestamp BETWEEN $__timeFrom() AND $__timeTo()\n  AND AssignmentConfidence IS NOT NULL\nORDER BY time ASC",
          "refId": "A"
        }
      ],
      "title": "Regime Assignment Confidence",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Count of UNKNOWN regime assignments vs known regimes",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "UNKNOWN" }, "properties": [{ "id": "color", "value": { "fixedColor": "dark-purple", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "Known" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] }
        ]
      },
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 27 },
      "id": 33,
      "options": {
        "displayLabels": ["percent", "name"],
        "legend": { "displayMode": "table", "placement": "right", "showLegend": true, "values": ["value"] },
        "pieType": "donut",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n    CASE WHEN RegimeLabel = -1 THEN 'UNKNOWN' ELSE 'Known' END AS RegimeType,\n    COUNT(*) AS Count\nFROM ACM_RegimeTimeline\nWHERE EquipID = $equipment\n  AND Timestamp BETWEEN $__timeFrom() AND $__timeTo()\nGROUP BY CASE WHEN RegimeLabel = -1 THEN 'UNKNOWN' ELSE 'Known' END",
          "refId": "A"
        }
      ],
      "title": "UNKNOWN vs Known Regimes",
      "type": "piechart"
    },
    {
      "datasource": { "type": "mssql", "uid": "${datasource}" },
      "description": "Regime distribution by label",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }
        },
        "overrides": []
      },
      "gridPos": { "h": 6, "w": 6, "x": 18, "y": 27 },
      "id": 34,
      "options": {
        "displayLabels": ["percent", "name"],
        "legend": { "displayMode": "table", "placement": "right", "showLegend": true, "values": ["value"] },
        "pieType": "donut",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "datasource": { "type": "mssql", "uid": "${datasource}" },
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n    CASE WHEN RegimeLabel = -1 THEN 'UNKNOWN' ELSE CAST(RegimeLabel AS NVARCHAR(10)) END AS Regime,\n    COUNT(*) AS Count\nFROM ACM_RegimeTimeline\nWHERE EquipID = $equipment\n  AND Timestamp BETWEEN $__timeFrom() AND $__timeTo()\nGROUP BY RegimeLabel\nORDER BY RegimeLabel",
          "refId": "A"

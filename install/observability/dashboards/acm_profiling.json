{
  "uid": "acm-profiling",
  "title": "ACM Profiling & Resources",
  "description": "CPU and memory profiling for ACM pipeline",
  "tags": ["acm", "profiling", "resources"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 3,
  "refresh": "30s",
  "time": { "from": "now-1h", "to": "now" },
  "templating": {
    "list": [
      {
        "name": "equipment",
        "label": "Equipment",
        "type": "query",
        "datasource": { "uid": "pyroscope-ds", "type": "grafana-pyroscope-datasource" },
        "query": "{\"labelSelector\":\"{service_name=\\\"acm\\\"}\",\"profileTypeId\":\"process_cpu:cpu:nanoseconds:cpu:nanoseconds\",\"refId\":\"A\",\"type\":\"label\",\"label\":\"equipment\"}",
        "refresh": 2,
        "includeAll": true,
        "multi": false,
        "current": { "text": "All", "value": "$__all" },
        "allValue": ""
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "CPU Flame Graph (Pyroscope)",
      "type": "row",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 2,
      "title": "CPU Profile",
      "description": "CPU profiling flame graph - shows where time is spent. Select equipment to filter.",
      "type": "flamegraph",
      "gridPos": { "x": 0, "y": 1, "w": 24, "h": 12 },
      "datasource": { "uid": "pyroscope-ds", "type": "grafana-pyroscope-datasource" },
      "targets": [
        {
          "refId": "A",
          "profileTypeId": "process_cpu:cpu:nanoseconds:cpu:nanoseconds",
          "labelSelector": "{service_name=\"acm\"${equipment:pipe}}",
          "queryType": "profile",
          "groupBy": []
        }
      ],
      "options": {}
    },
    {
      "id": 10,
      "title": "Memory Profiling",
      "type": "row",
      "gridPos": { "x": 0, "y": 13, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 11,
      "title": "Memory Usage Over Time",
      "description": "RSS memory by section",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 14, "w": 12, "h": 8 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "max by (section) (acm_memory_rss_mb)",
          "legendFormat": "{{section}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 10, "spanNulls": 300000 },
          "unit": "decmbytes",
          "min": 0
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 12,
      "title": "Peak Memory by Equipment",
      "description": "Maximum memory used per equipment",
      "type": "bargauge",
      "gridPos": { "x": 12, "y": 14, "w": 12, "h": 8 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "max by (equipment) (acm_memory_peak_mb)",
          "legendFormat": "{{equipment}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 500 },
              { "color": "red", "value": 1000 }
            ]
          },
          "unit": "decmbytes",
          "min": 0
        }
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "reduceOptions": { "calcs": ["max"], "fields": "", "values": false }
      }
    },
    {
      "id": 20,
      "title": "CPU Metrics",
      "type": "row",
      "gridPos": { "x": 0, "y": 22, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 21,
      "title": "CPU Usage Over Time",
      "description": "CPU percentage by section",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 23, "w": 12, "h": 8 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "avg by (section) (acm_cpu_percent)",
          "legendFormat": "{{section}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineWidth": 1, "fillOpacity": 20, "spanNulls": 300000 },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 22,
      "title": "Section Duration",
      "description": "Time spent in each pipeline section",
      "type": "timeseries",
      "gridPos": { "x": 12, "y": 23, "w": 12, "h": 8 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(acm_section_duration_seconds_sum[5m])",
          "legendFormat": "{{section}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "bars", "fillOpacity": 80, "stacking": { "mode": "normal" } },
          "unit": "s",
          "min": 0
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["sum"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 30,
      "title": "Disk I/O",
      "type": "row",
      "gridPos": { "x": 0, "y": 31, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 31,
      "title": "Disk Read (MB)",
      "description": "Disk read per section",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 32, "w": 12, "h": 6 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (section) (acm_disk_read_mb)",
          "legendFormat": "{{section}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 20, "spanNulls": 300000 },
          "unit": "decmbytes",
          "min": 0
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 32,
      "title": "Disk Write (MB)",
      "description": "Disk write per section",
      "type": "timeseries",
      "gridPos": { "x": 12, "y": 32, "w": 12, "h": 6 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (section) (acm_disk_write_mb)",
          "legendFormat": "{{section}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 20, "spanNulls": 300000 },
          "unit": "decmbytes",
          "min": 0
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 40,
      "title": "Timer Logs",
      "type": "row",
      "gridPos": { "x": 0, "y": 38, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 41,
      "title": "Section Timing Logs",
      "description": "Timer logs from pipeline sections",
      "type": "logs",
      "gridPos": { "x": 0, "y": 39, "w": 24, "h": 8 },
      "datasource": { "uid": "loki-ds", "type": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "{app=\"acm\", log_type=\"timer\"}",
          "queryType": "range"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": false,
        "wrapLogMessage": false,
        "prettifyLogMessage": true,
        "enableLogDetails": true,
        "sortOrder": "Descending"
      }
    },
    {
      "id": 50,
      "title": "Traces",
      "type": "row",
      "gridPos": { "x": 0, "y": 47, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 51,
      "title": "Recent Traces",
      "description": "View traces from Tempo to see span durations",
      "type": "table",
      "gridPos": { "x": 0, "y": 48, "w": 24, "h": 8 },
      "datasource": { "uid": "tempo-ds", "type": "tempo" },
      "targets": [
        {
          "refId": "A",
          "queryType": "traceql",
          "query": "{}"
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "options": { "showHeader": true, "cellHeight": "sm" }
    },
    {
      "id": 60,
      "title": "Resource Summary",
      "type": "row",
      "gridPos": { "x": 0, "y": 56, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 61,
      "title": "Current Memory",
      "type": "stat",
      "gridPos": { "x": 0, "y": 57, "w": 4, "h": 4 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [{ "refId": "A", "expr": "max(acm_memory_rss_mb)" }],
      "fieldConfig": {
        "defaults": {
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 500 }, { "color": "red", "value": 1000 }] },
          "unit": "decmbytes",
          "noValue": "0"
        }
      },
      "options": { "reduceOptions": { "calcs": ["last"] }, "colorMode": "value", "graphMode": "area" }
    },
    {
      "id": 62,
      "title": "Peak Memory",
      "type": "stat",
      "gridPos": { "x": 4, "y": 57, "w": 4, "h": 4 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [{ "refId": "A", "expr": "max(acm_memory_peak_mb)" }],
      "fieldConfig": {
        "defaults": {
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 500 }, { "color": "red", "value": 1000 }] },
          "unit": "decmbytes",
          "noValue": "0"
        }
      },
      "options": { "reduceOptions": { "calcs": ["max"] }, "colorMode": "value", "graphMode": "none" }
    },
    {
      "id": 63,
      "title": "Avg CPU",
      "type": "stat",
      "gridPos": { "x": 8, "y": 57, "w": 4, "h": 4 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [{ "refId": "A", "expr": "avg(acm_cpu_percent)" }],
      "fieldConfig": {
        "defaults": {
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 70 }, { "color": "red", "value": 90 }] },
          "unit": "percent",
          "noValue": "0"
        }
      },
      "options": { "reduceOptions": { "calcs": ["mean"] }, "colorMode": "value", "graphMode": "area" }
    },
    {
      "id": 64,
      "title": "Total Disk Read",
      "type": "stat",
      "gridPos": { "x": 12, "y": 57, "w": 4, "h": 4 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [{ "refId": "A", "expr": "sum(acm_disk_read_mb)" }],
      "fieldConfig": {
        "defaults": {
          "thresholds": { "mode": "absolute", "steps": [{ "color": "blue", "value": null }] },
          "unit": "decmbytes",
          "noValue": "0"
        }
      },
      "options": { "reduceOptions": { "calcs": ["last"] }, "colorMode": "value", "graphMode": "none" }
    },
    {
      "id": 65,
      "title": "Total Disk Write",
      "type": "stat",
      "gridPos": { "x": 16, "y": 57, "w": 4, "h": 4 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [{ "refId": "A", "expr": "sum(acm_disk_write_mb)" }],
      "fieldConfig": {
        "defaults": {
          "thresholds": { "mode": "absolute", "steps": [{ "color": "purple", "value": null }] },
          "unit": "decmbytes",
          "noValue": "0"
        }
      },
      "options": { "reduceOptions": { "calcs": ["last"] }, "colorMode": "value", "graphMode": "none" }
    },
    {
      "id": 66,
      "title": "Total Runs",
      "type": "stat",
      "gridPos": { "x": 20, "y": 57, "w": 4, "h": 4 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [{ "refId": "A", "expr": "sum(acm_runs_total)" }],
      "fieldConfig": {
        "defaults": {
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] },
          "unit": "none",
          "noValue": "0"
        }
      },
      "options": { "reduceOptions": { "calcs": ["last"] }, "colorMode": "value", "graphMode": "none" }
    }
  ],
  "annotations": { "list": [] }
}

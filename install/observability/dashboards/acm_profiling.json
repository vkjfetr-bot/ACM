{
  "uid": "acm-profiling",
  "title": "ACM Profiling & Resources",
  "description": "CPU and memory profiling for ACM pipeline",
  "tags": ["acm", "profiling", "resources"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 2,
  "refresh": "30s",
  "time": { "from": "now-1h", "to": "now" },
  "templating": {
    "list": [
      {
        "name": "equipment",
        "type": "query",
        "datasource": { "uid": "loki-ds", "type": "loki" },
        "query": "label_values({app=\"acm\"}, equipment)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": { "text": "All", "value": "$__all" }
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "CPU Profiling",
      "type": "row",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 2,
      "title": "CPU Usage Over Time",
      "description": "CPU percentage by section",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 1, "w": 12, "h": 8 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "avg by (section) (acm_cpu_percent)",
          "legendFormat": "{{section}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineWidth": 1, "fillOpacity": 20, "spanNulls": 300000 },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 3,
      "title": "Section Duration (seconds)",
      "description": "Time spent in each pipeline section",
      "type": "timeseries",
      "gridPos": { "x": 12, "y": 1, "w": 12, "h": 8 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(acm_section_duration_seconds_sum[5m])",
          "legendFormat": "{{section}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "bars", "fillOpacity": 80, "stacking": { "mode": "normal" } },
          "unit": "s",
          "min": 0
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["sum"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 10,
      "title": "Memory Profiling",
      "type": "row",
      "gridPos": { "x": 0, "y": 9, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 11,
      "title": "Memory Usage Over Time",
      "description": "RSS memory by section",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 10, "w": 12, "h": 8 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "max by (section) (acm_memory_rss_mb)",
          "legendFormat": "{{section}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 10, "spanNulls": 300000 },
          "unit": "decmbytes",
          "min": 0
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 12,
      "title": "Peak Memory by Equipment",
      "description": "Maximum memory used per equipment",
      "type": "bargauge",
      "gridPos": { "x": 12, "y": 10, "w": 12, "h": 8 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "max by (equipment) (acm_memory_peak_mb)",
          "legendFormat": "{{equipment}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 500 },
              { "color": "red", "value": 1000 }
            ]
          },
          "unit": "decmbytes",
          "min": 0
        }
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "reduceOptions": { "calcs": ["max"], "fields": "", "values": false }
      }
    },
    {
      "id": 20,
      "title": "Timer Logs",
      "type": "row",
      "gridPos": { "x": 0, "y": 18, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 21,
      "title": "Section Timing Logs",
      "description": "Timer logs from pipeline sections",
      "type": "logs",
      "gridPos": { "x": 0, "y": 19, "w": 24, "h": 8 },
      "datasource": { "uid": "loki-ds", "type": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "{app=\"acm\", log_type=\"timer\"}",
          "queryType": "range"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": false,
        "wrapLogMessage": false,
        "prettifyLogMessage": true,
        "enableLogDetails": true,
        "sortOrder": "Descending"
      }
    },
    {
      "id": 30,
      "title": "Disk I/O",
      "type": "row",
      "gridPos": { "x": 0, "y": 27, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 31,
      "title": "Disk Read (MB)",
      "description": "Disk read per section",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 28, "w": 12, "h": 6 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (section) (acm_disk_read_mb)",
          "legendFormat": "{{section}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 20, "spanNulls": 300000 },
          "unit": "decmbytes",
          "min": 0
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 32,
      "title": "Disk Write (MB)",
      "description": "Disk write per section",
      "type": "timeseries",
      "gridPos": { "x": 12, "y": 28, "w": 12, "h": 6 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (section) (acm_disk_write_mb)",
          "legendFormat": "{{section}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 20, "spanNulls": 300000 },
          "unit": "decmbytes",
          "min": 0
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 40,
      "title": "Distributed Tracing",
      "type": "row",
      "gridPos": { "x": 0, "y": 34, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 41,
      "title": "Section Traces",
      "description": "View traces from Tempo to see span durations",
      "type": "table",
      "gridPos": { "x": 0, "y": 35, "w": 24, "h": 8 },
      "datasource": { "uid": "tempo-ds", "type": "tempo" },
      "targets": [
        {
          "refId": "A",
          "queryType": "traceql",
          "query": "{}"
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "traceID" },
            "properties": [
              { "id": "links", "value": [{ "title": "View Trace", "url": "/explore?left={\"queries\":[{\"refId\":\"A\",\"datasource\":{\"type\":\"tempo\",\"uid\":\"tempo-ds\"},\"queryType\":\"traceql\",\"query\":\"${__value.raw}\"}]}" }] }
            ]
          }
        ]
      },
      "options": { "showHeader": true, "cellHeight": "sm" }
    },
    {
      "id": 50,
      "title": "Resource Summary",
      "type": "row",
      "gridPos": { "x": 0, "y": 43, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 51,
      "title": "Current Memory (MB)",
      "type": "stat",
      "gridPos": { "x": 0, "y": 44, "w": 4, "h": 4 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [{ "refId": "A", "expr": "max(acm_memory_rss_mb)" }],
      "fieldConfig": {
        "defaults": {
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 500 }, { "color": "red", "value": 1000 }] },
          "unit": "decmbytes",
          "noValue": "No data"
        }
      },
      "options": { "reduceOptions": { "calcs": ["last"] }, "colorMode": "value", "graphMode": "area" }
    },
    {
      "id": 52,
      "title": "Peak Memory (MB)",
      "type": "stat",
      "gridPos": { "x": 4, "y": 44, "w": 4, "h": 4 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [{ "refId": "A", "expr": "max(acm_memory_peak_mb)" }],
      "fieldConfig": {
        "defaults": {
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 500 }, { "color": "red", "value": 1000 }] },
          "unit": "decmbytes",
          "noValue": "No data"
        }
      },
      "options": { "reduceOptions": { "calcs": ["max"] }, "colorMode": "value", "graphMode": "none" }
    },
    {
      "id": 53,
      "title": "Avg CPU (%)",
      "type": "stat",
      "gridPos": { "x": 8, "y": 44, "w": 4, "h": 4 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [{ "refId": "A", "expr": "avg(acm_cpu_percent)" }],
      "fieldConfig": {
        "defaults": {
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 70 }, { "color": "red", "value": 90 }] },
          "unit": "percent",
          "noValue": "No data"
        }
      },
      "options": { "reduceOptions": { "calcs": ["mean"] }, "colorMode": "value", "graphMode": "area" }
    },
    {
      "id": 54,
      "title": "Total Batches",
      "type": "stat",
      "gridPos": { "x": 12, "y": 44, "w": 4, "h": 4 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [{ "refId": "A", "expr": "sum(acm_batches_processed_total)" }],
      "fieldConfig": {
        "defaults": {
          "thresholds": { "mode": "absolute", "steps": [{ "color": "blue", "value": null }] },
          "unit": "none",
          "noValue": "No data"
        }
      },
      "options": { "reduceOptions": { "calcs": ["last"] }, "colorMode": "value", "graphMode": "none" }
    },
    {
      "id": 55,
      "title": "Rows Processed",
      "type": "stat",
      "gridPos": { "x": 16, "y": 44, "w": 4, "h": 4 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [{ "refId": "A", "expr": "sum(acm_rows_processed_total)" }],
      "fieldConfig": {
        "defaults": {
          "thresholds": { "mode": "absolute", "steps": [{ "color": "purple", "value": null }] },
          "unit": "short",
          "noValue": "No data"
        }
      },
      "options": { "reduceOptions": { "calcs": ["last"] }, "colorMode": "value", "graphMode": "area" }
    },
    {
      "id": 56,
      "title": "Errors",
      "type": "stat",
      "gridPos": { "x": 20, "y": 44, "w": 4, "h": 4 },
      "datasource": { "uid": "prometheus-ds", "type": "prometheus" },
      "targets": [{ "refId": "A", "expr": "sum(acm_errors_total)" }],
      "fieldConfig": {
        "defaults": {
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "red", "value": 1 }] },
          "unit": "none",
          "noValue": "0"
        }
      },
      "options": { "reduceOptions": { "calcs": ["last"] }, "colorMode": "background", "graphMode": "none" }
    }
  ],
  "annotations": { "list": [] }
}

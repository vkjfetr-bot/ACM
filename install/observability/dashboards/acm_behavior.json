{
  "uid": "acm-behavior",
  "title": "ACM Behavior",
  "description": "Monitor ACM equipment health, anomaly detection, RUL predictions, and profiling",
  "tags": ["acm", "health", "anomaly", "rul", "profiling"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 2,
  "refresh": "1m",
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "panels": [
    {
      "id": 100,
      "title": "Run Statistics",
      "type": "row",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 1,
      "title": "Successful Runs",
      "type": "stat",
      "gridPos": { "x": 0, "y": 1, "w": 4, "h": 4 },
      "datasource": { "uid": "loki-ds", "type": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "count_over_time({app=\"acm\"} |~ `outcome=OK` [$__range])"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          },
          "color": { "mode": "thresholds" },
          "unit": "none"
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["sum"], "fields": "", "values": false },
        "textMode": "value_and_name",
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 2,
      "title": "Rows Processed",
      "type": "stat",
      "gridPos": { "x": 4, "y": 1, "w": 4, "h": 4 },
      "datasource": { "uid": "loki-ds", "type": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum_over_time({app=\"acm\"} |~ `rows_in=\\d+` | regexp `rows_in=(?P<rows>\\d+)` | unwrap rows [$__range])"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "blue", "value": null }]
          },
          "color": { "mode": "thresholds" },
          "unit": "short"
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["sum"], "fields": "", "values": false },
        "textMode": "value_and_name",
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 3,
      "title": "Episodes Detected",
      "type": "stat",
      "gridPos": { "x": 8, "y": 1, "w": 4, "h": 4 },
      "datasource": { "uid": "loki-ds", "type": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum_over_time({app=\"acm\"} |~ `Detected \\d+ anomaly episodes` | regexp `Detected (?P<episodes>\\d+) anomaly episodes` | unwrap episodes [$__range])"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "orange", "value": 50 },
              { "color": "red", "value": 100 }
            ]
          },
          "color": { "mode": "thresholds" },
          "unit": "none"
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["sum"], "fields": "", "values": false },
        "textMode": "value_and_name",
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 4,
      "title": "Errors",
      "type": "stat",
      "gridPos": { "x": 12, "y": 1, "w": 4, "h": 4 },
      "datasource": { "uid": "loki-ds", "type": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "count_over_time({app=\"acm\", level=\"error\"} [$__range])"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          },
          "color": { "mode": "thresholds" },
          "unit": "none"
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["sum"], "fields": "", "values": false },
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "none"
      }
    },
    {
      "id": 40,
      "title": "NOOP Runs",
      "type": "stat",
      "gridPos": { "x": 16, "y": 1, "w": 4, "h": 4 },
      "datasource": { "uid": "loki-ds", "type": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "count_over_time({app=\"acm\"} |~ `outcome=NOOP` [$__range])"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "yellow", "value": null }
            ]
          },
          "color": { "mode": "thresholds" },
          "unit": "none"
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["sum"], "fields": "", "values": false },
        "textMode": "value_and_name",
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 41,
      "title": "Models Saved",
      "type": "stat",
      "gridPos": { "x": 20, "y": 1, "w": 4, "h": 4 },
      "datasource": { "uid": "loki-ds", "type": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "count_over_time({app=\"acm\"} |~ `OK Committed.*models to SQL ModelRegistry` [$__range])"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "purple", "value": null }]
          },
          "color": { "mode": "thresholds" },
          "unit": "none"
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["sum"], "fields": "", "values": false },
        "textMode": "value_and_name",
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 5,
      "title": "Pipeline Activity Timeline",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 5, "w": 24, "h": 6 },
      "datasource": { "uid": "loki-ds", "type": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "count_over_time({app=\"acm\"} |~ `outcome=OK` [$__interval])",
          "legendFormat": "Successful Runs"
        },
        {
          "refId": "B",
          "expr": "count_over_time({app=\"acm\"} |~ `outcome=NOOP` [$__interval])",
          "legendFormat": "NOOP Runs"
        },
        {
          "refId": "C",
          "expr": "count_over_time({app=\"acm\", level=\"error\"} [$__interval])",
          "legendFormat": "Errors"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": 3600000
          },
          "color": { "mode": "palette-classic" }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Errors" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "NOOP Runs" }, "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "Successful Runs" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] }
        ]
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 101,
      "title": "Logs & Traces",
      "type": "row",
      "gridPos": { "x": 0, "y": 11, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 6,
      "title": "Recent ACM Activity",
      "type": "logs",
      "gridPos": { "x": 0, "y": 12, "w": 16, "h": 10 },
      "datasource": { "uid": "loki-ds", "type": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "{app=\"acm\"} |~ `(Started RunID|outcome=|Detected.*episodes|RUL P50)`",
          "queryType": "range"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "wrapLogMessage": true,
        "enableLogDetails": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none"
      }
    },
    {
      "id": 50,
      "title": "Recent Traces",
      "type": "traces",
      "gridPos": { "x": 16, "y": 12, "w": 8, "h": 10 },
      "datasource": { "uid": "tempo-ds", "type": "tempo" },
      "targets": [
        {
          "refId": "A",
          "queryType": "nativeSearch",
          "serviceName": "acm-pipeline",
          "limit": 10
        }
      ],
      "options": {}
    },
    {
      "id": 102,
      "title": "Health & Forecasting",
      "type": "row",
      "gridPos": { "x": 0, "y": 22, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 7,
      "title": "Health & RUL Updates",
      "type": "logs",
      "gridPos": { "x": 0, "y": 23, "w": 12, "h": 8 },
      "datasource": { "uid": "loki-ds", "type": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "{app=\"acm\", component=~\"FORECAST|RUL|HealthTracker|DegradationModel|RULEstimator\"}",
          "queryType": "range"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "wrapLogMessage": true,
        "enableLogDetails": true,
        "sortOrder": "Descending"
      }
    },
    {
      "id": 51,
      "title": "CPU Profile (Pyroscope)",
      "type": "flamegraph",
      "gridPos": { "x": 12, "y": 23, "w": 12, "h": 8 },
      "datasource": { "uid": "pyroscope-ds", "type": "grafana-pyroscope-datasource" },
      "targets": [
        {
          "refId": "A",
          "profileTypeId": "process_cpu:cpu:nanoseconds:cpu:nanoseconds",
          "labelSelector": "{service_name=\"acm\"}",
          "queryType": "profile"
        }
      ],
      "options": {}
    },
    {
      "id": 103,
      "title": "Detector Performance",
      "type": "row",
      "gridPos": { "x": 0, "y": 31, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 60,
      "title": "Detector Logs",
      "type": "logs",
      "gridPos": { "x": 0, "y": 32, "w": 24, "h": 6 },
      "datasource": { "uid": "loki-ds", "type": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "{app=\"acm\", component=~\"AR1|PCA|IFOREST|GMM|OMR|MODEL\"}",
          "queryType": "range"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "wrapLogMessage": true,
        "enableLogDetails": true,
        "sortOrder": "Descending"
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "equipment",
        "type": "query",
        "datasource": { "uid": "loki-ds", "type": "loki" },
        "query": "label_values({app=\"acm\"}, equipment)",
        "refresh": 2,
        "includeAll": true,
        "multi": false,
        "current": { "text": "All", "value": "$__all" }
      }
    ]
  },
  "annotations": {
    "list": []
  }
}
